				float3 temp_cast_0 = (1.0).xxx;
				
				float2 uv_BaseMap = i.ase_texcoord3.xy * _BaseMap_ST.xy + _BaseMap_ST.zw;
				float4 tex2DNode1 = tex2D( _BaseMap, uv_BaseMap );
				float clampResult8 = clamp( ( tex2DNode1.a * _Opacity ) , 0.0 , 1.0 );
				float Opacity19 = clampResult8;
				float temp_output_2_0_g7 = Opacity19;
